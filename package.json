{
  "name": "simplecheckout-sdk",
  "version": "1.2.1",
  "description": "A modern, secure JavaScript SDK for credit card data collection with PCI-compliant handling and customizable styling",
  "main": "dist/SimpleCheckout.js",
  "types": "dist/SimpleCheckout.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/SimpleCheckout.js",
      "types": "./dist/SimpleCheckout.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:types": "tsc --declaration --emitDeclarationOnly",
    "build:package": "npm run build && npm run build:types",
    "preview": "vite preview",
    "start": "vite",
    "playground": "cd playground && npm start",
    "playground:dev": "cd playground && npm run dev",
    "cvc:dev": "cd cvc-verification && npm run dev",
    "type-check": "tsc --noEmit",
    "pack:test": "npm run build:package && npm pack",
    "prepublishOnly": "npm run build:package",
    "release": "./scripts/release.sh"
  },
  "dependencies": {
    "@vgs/collect-js": "^0.7.2"
  },
  "devDependencies": {
    "vite": "^5.0.0",
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0"
  },
  "keywords": [
    "payment",
    "form",
    "iframe",
    "simplecheckout",
    "credit-card",
    "secure-payments",
    "pci-compliant",
    "payment-processing",
    "checkout",
    "sdk"
  ],
  "author": "SimpleCheckout Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fewsats/simplecheckout-ts.git"
  },
  "homepage": "https://github.com/Fewsats/simplecheckout-ts#readme",
  "bugs": {
    "url": "https://github.com/Fewsats/simplecheckout-ts/issues"
  }
}
